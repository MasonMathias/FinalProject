# Firebase API Keys - Template File
# Copy this file to .env and fill in your actual values from Firebase Console

# Web API Key
FIREBASE_WEB_API_KEY=your_web_api_key_here

# Android API Key
FIREBASE_ANDROID_API_KEY=your_android_api_key_here

# iOS API Key
FIREBASE_IOS_API_KEY=your_ios_api_key_here

# Common Firebase Configuration
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
FIREBASE_STORAGE_BUCKET=your_storage_bucket
FIREBASE_AUTH_DOMAIN=your_auth_domain

# Web App IDs
FIREBASE_WEB_APP_ID=your_web_app_id
FIREBASE_WEB_MEASUREMENT_ID=your_web_measurement_id

# Android App ID
FIREBASE_ANDROID_APP_ID=your_android_app_id

# iOS App IDs
FIREBASE_IOS_APP_ID=your_ios_app_id
FIREBASE_IOS_BUNDLE_ID=your_ios_bundle_id

# Windows App ID
FIREBASE_WINDOWS_APP_ID=your_windows_app_id
FIREBASE_WINDOWS_MEASUREMENT_ID=your_windows_measurement_id
